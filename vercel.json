{
  "version": 2,
  "builds": [
    {
      "src": "dist/*/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/blog-pessoal"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    }
  ]
}